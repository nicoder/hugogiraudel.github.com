{
  "autoPrefixer": {
    "active": true,
    "browsers": [
      "last 1 version",
      "> 1%",
      "ie 8"
    ],
    "sourceMap": false
  },
  "debug": false,
  "lintScripts": true,
  "optimiseImages": true,
  "optimiseSVG": true,
  "preprocessorLocations": "assets",
  "preprocessorScriptOutput": "assets/js",
  "preprocessorStyleOutput": "assets/css",
  "projectDescription": "I write about Sass. Mostly.",
  "projectName": "HugoGiraudel.com",
  "remoteDebugPort": 8080,
  "routes": [
    {
      "redirect": null,
      "route": ":year/:month/:day/:url",
      "template": "post"
    },
    {
      "redirect": null,
      "route": "page:page",
      "template": "index"
    },
    {
      "redirect": null,
      "route": "tag/:tag",
      "template": "index"
    },
    {
      "redirect": null,
      "route": "feeds/feed.xml",
      "template": "rss"
    }
  ],
  "sitemap": false,
  "templateDefaultLayout": "layout",
  "templateDefaultPage": "index",
  "templateDirectory": "templates",
  "type": "mixture",
  "convertHtml": {
    "extension": ".html",
    "path": null,
    "ignoreUrls": "/article/, /post, /collections",
    "relativePaths": false,
    "default": "index"
  }
}